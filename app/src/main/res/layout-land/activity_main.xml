<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/botonPreg1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="68dp"
        android:text="@string/Cap"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/botonPreg4"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonPreg4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="68dp"
        android:text="@string/Nier"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonPreg2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="68dp"
        android:text="@string/Serp"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/botonPreg3"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonPreg3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="68dp"
        android:text="@string/MiddleEarth"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/botonPreg1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/PregActual"
        android:layout_width="217dp"
        android:layout_height="25dp"
        android:text="@string/PregActual"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.461" />

    <Button
        android:id="@+id/botonPreg5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="68dp"
        android:text="@string/Yautja"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/botonPreg2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonRes1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="240dp"
        android:text="@string/ResVacia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="217dp"
        android:layout_height="44dp"
        android:layout_marginTop="80dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/Selec"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonRes2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="240dp"
        android:text="@string/ResVacia"
        app:layout_constraintStart_toEndOf="@+id/botonRes1"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonRes3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="240dp"
        android:text="@string/ResVacia"
        app:layout_constraintStart_toEndOf="@+id/botonRes2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonRes4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="240dp"
        android:text="@string/ResVacia"
        app:layout_constraintStart_toEndOf="@+id/botonRes3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="217dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:text="@string/Cate"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>